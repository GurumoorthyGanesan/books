        "parent_table": "block",
        "properties": {
          "title": [
            [
              "View in MVC is tightly coupled with the Controller, the View part of the MVP consists of both UIViews and UIViewController"
            ]
          ]
        },
        "type": "bulleted_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "View in MVC is tightly coupled with the Controller, the View part of the MVP consists of both UIViews and UIViewController"
          }
        ]
      },
