          "title": [
            [
              "extension UIView {\n    @discardableResult\n    public func setAsCircle() -\u003e Self {\n        self.clipsToBounds = true\n        let frameSize = self.frame.size\n        self.layer.cornerRadius = min(frameSize.width, frameSize.height) / 2.0\n        return self\n    }\n}"
            ]
          ]
        },
