          "title": [
            [
              "func adjustedColorForColor( c: UIColor, var percent: CGFloat) -\u003e UIColor {\n    if percent \u003c 0 {\n        percent = 0\n    }\n\n    var r,g,b,a: CGFloat\n    r = 0.0\n    g = 0.0\n    b = 0.0\n    a = 0.0\n\n    if c.getRed(\u0026r, green: \u0026g, blue: \u0026b, alpha: \u0026a) {\n        return UIColor(red: max(r * percent, 0.0), green: max(g * percent, 0.0), blue: max(b * percent, 0.0), alpha: a)\n    }\n\n    return UIColor()\n}"
            ]
          ]
        },
