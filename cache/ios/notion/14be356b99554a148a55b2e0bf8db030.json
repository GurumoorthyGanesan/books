            ]
          ],
          "title": [
            [
              "+ (UIColor *)darkerColorForColor:(UIColor *)c\n{\n    CGFloat r, g, b, a;\n    if ([c getRed:\u0026r green:\u0026g blue:\u0026b alpha:\u0026a])\n        return [UIColor colorWithRed:MAX(r - 0.2, 0.0)\n                               green:MAX(g - 0.2, 0.0)\n                                blue:MAX(b - 0.2, 0.0)\n                               alpha:a];\n    return nil;\n}"
            ]
          ]
        },
