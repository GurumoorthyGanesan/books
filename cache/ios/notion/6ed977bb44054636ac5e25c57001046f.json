          "title": [
            [
              "+(NSTimeInterval)getUTCOffSetIntervalWithCurrentTimeZone:(NSTimeZone *)current forDate:(NSDate *)date  {\n    NSTimeZone *utcTimeZone = [NSTimeZone timeZoneWithAbbreviation:@\"UTC\"];\n    NSInteger currentGMTOffset = [current secondsFromGMTForDate:date];\n    NSInteger gmtOffset = [utcTimeZone secondsFromGMTForDate:date];\n    NSTimeInterval gmtInterval = currentGMTOffset - gmtOffset;\n    return gmtInterval;\n}"
            ]
          ]
        },
