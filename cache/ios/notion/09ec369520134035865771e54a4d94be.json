          "title": [
            [
              "@implementation NSData (HexRepresentation)\n\n- (NSString *)hexString {\n    const unsigned char *bytes = (const unsigned char *)self.bytes;\n    NSMutableString *hex = [NSMutableString new];\n    for (NSInteger i = 0; i \u003c self.length; i++) {\n        [hex appendFormat:@\"%02x\", bytes[i]];\n    }\n    return [hex copy];\n}\n\n@end"
            ]
          ]
        },
