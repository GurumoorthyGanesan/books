            [
              "import datetime\n\n# The size of each step in days\nday_delta = datetime.timedelta(days=1)\n\nstart_date = datetime.date.today()\nend_date = start_date + 7*day_delta\n\nfor i in range((end_date - start_date).days):\n    print(start_date + i*day_delta)"
