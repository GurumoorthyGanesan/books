        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "wget --no-check-certificate https://www.python.org/ftp/python/2.7.X/Python-2.7.X.tgz\ntar -xzf Python-2.7.X.tgz  \ncd Python-2.7.X\n./configure  \nmake  \nsudo make install"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "wget --no-check-certificate https://www.python.org/ftp/python/2.7.X/Python-2.7.X.tgz\ntar -xzf Python-2.7.X.tgz  \ncd Python-2.7.X\n./configure  \nmake  \nsudo make install",
        "code_language": "Plain Text"
