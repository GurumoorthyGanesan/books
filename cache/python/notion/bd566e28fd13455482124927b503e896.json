        "inline_content": [
          {
            "Text": "To be able to serialize the datetime object properly, we need to write custom code for how to convert it:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550304896613,
        "id": "29bceb34-2494-4ab9-9d0e-dc0f69586cee",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550305560000,
        "parent_id": "bd566e28-fd13-4554-8212-4927b503e896",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Python"
            ]
          ],
          "title": [
            [
              "class DatetimeJSONEncoder(json.JSONEncoder):\n\tdef default(self, obj):\n\t\ttry:\n\t\t\treturn obj.isoformat()\n\t\texcept AttributeError:\n\t\t# obj has no isoformat method; let the builtin JSON encoder handle it\n\t\treturn super(DatetimeJSONEncoder, self).default(obj)\n\t\t"
            ]
          ]
        },
        "type": "code",
        "version": 10,
        "code": "class DatetimeJSONEncoder(json.JSONEncoder):\n\tdef default(self, obj):\n\t\ttry:\n\t\t\treturn obj.isoformat()\n\t\texcept AttributeError:\n\t\t# obj has no isoformat method; let the builtin JSON encoder handle it\n\t\treturn super(DatetimeJSONEncoder, self).default(obj)\n\t\t",
        "code_language": "Python"
      },
