          "title": [
            [
              "import itertools\n\noptions = {\n\t“x”: [“a”, “b”],\n\t“y”: [10, 20, 30]\n}\n\nkeys = options.keys()\nvalues = (options[key] for key in keys)\ncombinations = [dict(zip(keys, combination)) for combination in itertools.product(*values)]\nprint combinations"
            ]
          ]
        },
        "type": "code",
