          "title": [
            [
              "# Good examples, using implicit truth testing\nif attr:\n    # do something\n\nif not attr:\n    # do something\n\n# Bad examples, using specific types\nif attr == 1:\n    # do something\n\nif attr == True:\n    # do something\n\nif attr != '':\n    # do something\n\n# If you are looking to specifically check for None, use 'is' or 'is not'\nif attr is None:\n    # do something"
            ]
          ]
        },
        "type": "code",
        "version": 1,
