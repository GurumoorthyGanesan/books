          "title": [
            [
              "class SingletonType(type):\n    def __call__(cls, *args, **kwargs):\n        try:\n            return cls.__instance\n        except AttributeError:\n            cls.__instance = super(SingletonType, cls).__call__(*args, **kwargs)\n            return cls.__instance"
