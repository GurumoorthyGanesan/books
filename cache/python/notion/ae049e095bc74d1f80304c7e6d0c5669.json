        "properties": {
          "language": [
            [
              "Python"
            ]
          ],
          "title": [
            [
              "import re\n\ndef is_allowed(string):\n    characherRegex = re.compile(r'[^a-zA-Z0-9.]')\n    string = characherRegex.search(string)\n    return not bool(string)\n    \nprint (is_allowed(\"abyzABYZ0099\")) \n# Out: 'True'\n\nprint (is_allowed(\"#*@#$%^\")) \n# Out: 'False'"
            ]
          ]
