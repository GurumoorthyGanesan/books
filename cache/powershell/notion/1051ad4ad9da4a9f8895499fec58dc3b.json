        "properties": {
          "title": [
            [
              "Powershell naming system has quite strict rules of naming cmdlets (Verb-Noun template; see [topic not yet created] for more information). But it is not really convenient to write "
            ],
            [
              "Get-ChildItems",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " every time you want to list files in directory interactively."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Powershell naming system has quite strict rules of naming cmdlets (Verb-Noun template; see [topic not yet created] for more information). But it is not really convenient to write "
          },
          {
            "Text": "Get-ChildItems",
            "AttrFlags": 2
          },
          {
            "Text": " every time you want to list files in directory interactively."
          }
        ]
      },
