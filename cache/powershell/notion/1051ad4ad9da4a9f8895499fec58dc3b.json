        "properties": {
          "title": [
            [
              "Therefore Powershell enables using shortcuts - aliases - instead of cmdlet names."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Therefore Powershell enables using shortcuts - aliases - instead of cmdlet names."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
