        "parent_table": "block",
        "properties": {
          "title": [
            [
              "You can write "
            ],
            [
              "ls",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", "
            ],
            [
              "dir",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " or "
            ],
            [
              "gci",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " instead of "
            ],
            [
              "Get-ChildItem",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " and get the same result. Alias is equivalent to its cmdlet."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "You can write "
          },
          {
            "Text": "ls",
            "AttrFlags": 2
          },
          {
            "Text": ", "
          },
          {
            "Text": "dir",
            "AttrFlags": 2
          },
          {
            "Text": " or "
          },
          {
            "Text": "gci",
            "AttrFlags": 2
          },
          {
            "Text": " instead of "
          },
          {
            "Text": "Get-ChildItem",
            "AttrFlags": 2
          },
          {
            "Text": " and get the same result. Alias is equivalent to its cmdlet."
          }
        ]
