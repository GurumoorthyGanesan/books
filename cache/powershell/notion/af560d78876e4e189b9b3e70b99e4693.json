        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$newCollection = @()\n$newCollection += New-Object -TypeName PSObject -Property @{\n    Name = $env:username\n    ID = 12\n    Address = $null\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "$newCollection = @()\n$newCollection += New-Object -TypeName PSObject -Property @{\n    Name = $env:username\n    ID = 12\n    Address = $null\n}",
        "code_language": "Plain Text"
      },
