        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$newObject = New-Object -TypeName PSObject -Property @{\n    Name = $env:username\n    ID = 12\n    Address = $null\n}\n\n# Returns\nPS\u003e $newObject\nName ID Address\n---- -- -------\nnem  12"
            ]
          ]
        },
