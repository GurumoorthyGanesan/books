        "properties": {
          "title": [
            [
              "A temporary workaround for execution policy can also be achieved by running the Powershell executable and passing any valid policy as "
            ],
            [
              "-ExecutionPolicy",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " parameter. The policy is in effect only during process’ lifetime, so no administrative access to the registry is needed."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "A temporary workaround for execution policy can also be achieved by running the Powershell executable and passing any valid policy as "
          },
          {
            "Text": "-ExecutionPolicy",
            "AttrFlags": 2
          },
          {
            "Text": " parameter. The policy is in effect only during process’ lifetime, so no administrative access to the registry is needed."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
