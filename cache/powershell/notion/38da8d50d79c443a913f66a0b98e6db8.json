        "properties": {
          "title": [
            [
              "In most cases, the input of the pipeline will be an array of objects. Although the behavior of the "
            ],
            [
              "PROCESS{}",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " block may seem similar to the "
            ],
            [
              "foreach{}",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " block, skipping an element in the array requires a different process."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "In most cases, the input of the pipeline will be an array of objects. Although the behavior of the "
          },
          {
            "Text": "PROCESS{}",
            "AttrFlags": 2
          },
          {
            "Text": " block may seem similar to the "
          },
          {
            "Text": "foreach{}",
            "AttrFlags": 2
          },
          {
            "Text": " block, skipping an element in the array requires a different process."
          }
        ]
