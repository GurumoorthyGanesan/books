        "inline_content": [
          {
            "Text": "More advanced usage of the pipeline allows us to pipe the output of a cmdlet into a foreach loop:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1552098409346,
        "id": "1483729b-add0-4179-bcf2-74a6183becdd",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1552098409346,
        "parent_id": "ea4444c6-8290-4455-8012-a1b42602351b",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "Get-ChildItem | ForEach-Object {\n    Copy-Item -Path $_.FullName -destination C:\\NewDirectory\\ \n}\n\n#This may be shortened to:\ngci | % { Copy $_.FullName C:\\NewDirectory\\ }"
            ]
          ]
        },
        "type": "code",
