          "title": [
            [
              "try\n{\n    $session = New-PSsession -Computername \"RemoteMachineName\"\n    Invoke-Command -Session $session -ScriptBlock {write-host \"This is running on $ENV:ComputerName\"}\n}\ncatch\n{\n    Write-Output \"ERROR: $_\"\n}\nfinally\n{\n    if ($session)\n    {\n        Remove-PSSession $session\n    }\n}"
            ]
