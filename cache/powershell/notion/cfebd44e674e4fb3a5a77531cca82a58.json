        "properties": {
          "title": [
            [
              "You can also add properties with Select-Object Cmdlet (so called calculated properties):"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "You can also add properties with Select-Object Cmdlet (so called calculated properties):"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1552098468844,
        "id": "9d7e2794-44e9-4202-8496-08b6ac2f569f",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1552098468844,
        "parent_id": "cfebd44e-674e-4fb3-a5a7-7531cca82a58",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$newObject = $Object | Select-Object *, @{label='SomeOtherProp'; expression={'Another value'}}\n\n# Returns\nPS\u003e $newObject\nName ID Address SomeNewProp SomeOtherProp\n---- -- ------- ----------- -------------\nnem  12         A value     Another value"
            ]
          ]
        },
        "type": "code",
