        "properties": {
          "title": [
            [
              "If you’d like to add properties to an existing object, you can use the Add-Member cmdlet. With PSObjects, values are kept in a type of “Note Properties”"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "If you’d like to add properties to an existing object, you can use the Add-Member cmdlet. With PSObjects, values are kept in a type of “Note Properties”"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1552098468844,
        "id": "aac39568-df53-4f47-a4f7-7322241aac06",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1552098468844,
        "parent_id": "cfebd44e-674e-4fb3-a5a7-7531cca82a58",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$object = New-Object -TypeName PSObject -Property @{\n        Name = $env:username\n        ID = 12\n        Address = $null\n    }\n\nAdd-Member -InputObject $object -Name \"SomeNewProp\" -Value \"A value\" -MemberType NoteProperty\n\n# Returns\nPS\u003e $Object\nName ID Address SomeNewProp\n---- -- ------- -----------\nnem  12         A value"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "$object = New-Object -TypeName PSObject -Property @{\n        Name = $env:username\n        ID = 12\n        Address = $null\n    }\n\nAdd-Member -InputObject $object -Name \"SomeNewProp\" -Value \"A value\" -MemberType NoteProperty\n\n# Returns\nPS\u003e $Object\nName ID Address SomeNewProp\n---- -- ------- -----------\nnem  12         A value",
        "code_language": "Plain Text"
      },
