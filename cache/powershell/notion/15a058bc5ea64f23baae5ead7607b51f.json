        "parent_table": "block",
        "properties": {
          "title": [
            [
              "We can use the "
            ],
            [
              "ValidateSet",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " parameter attribute to restrict this. It has the additional benefit of allowing tab completion when setting this argument (in some environments)."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "We can use the "
          },
