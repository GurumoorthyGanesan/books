          ],
          "title": [
            [
              "is_alive() {\n    ping -c1 \"$1\" \u0026\u003e /dev/null\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "is_alive() {\n    ping -c1 \"$1\" \u0026\u003e /dev/null\n}",
        "code_language": "Plain Text"
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1551930001906,
        "id": "89abd8b3-ea45-4646-bd70-2330a57066fc",
