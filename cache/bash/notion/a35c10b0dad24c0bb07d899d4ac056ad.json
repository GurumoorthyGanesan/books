          "title": [
            [
              "#!/bin/bash\ndr=''\ninstall=false\n\nskip=false\nfor op in \"$@\";do\n    if $skip;then skip=false;continue;fi\n    case \"$op\" in\n        -v|--version)\n            echo \"$ver_info\"\n            shift\n            exit 0\n            ;;\n        -h|--help)\n            echo \"$help\"\n            shift\n            exit 0\n            ;;\n        -dr|--doc-root)\n            shift\n            if [[ \"$1\" != \"\" ]]; then\n                dr=\"${1/%\\//}\"\n                shift\n                skip=true\n            else\n                echo \"E: Arg missing for -dr option\"\n                exit 1\n            fi\n            ;;\n        -i|--install)\n            install=true\n            shift\n            ;;\n        -*)\n            echo \"E: Invalid option: $1\"\n            shift\n            exit 1\n            ;;\n    esac\ndone"
            ]
          ]
        },
