        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Unlike many languages like "
            ],
            [
              "C",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " where memory is explicitly allocated for structs and variables and such (and thus must be deallocated), "
            ],
            [
              "bash",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " handles this under the covers. In most cases, we don’t have to clean up anything before leaving the function. A "
            ],
            [
              "return",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " statement will deallocate everything local to the function and pickup execution at the return address on the stack."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Unlike many languages like "
          },
          {
            "Text": "C",
            "AttrFlags": 2
          },
          {
            "Text": " where memory is explicitly allocated for structs and variables and such (and thus must be deallocated), "
          },
          {
            "Text": "bash",
            "AttrFlags": 2
          },
          {
            "Text": " handles this under the covers. In most cases, we don’t have to clean up anything before leaving the function. A "
          },
          {
            "Text": "return",
            "AttrFlags": 2
          },
          {
            "Text": " statement will deallocate everything local to the function and pickup execution at the return address on the stack."
          }
        ]
