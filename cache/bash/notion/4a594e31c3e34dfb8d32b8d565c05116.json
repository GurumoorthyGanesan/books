        "properties": {
          "title": [
            [
              "There are two different ways of performing this expansion, which differ in whether the relevant variable is empty or unset. Using "
            ],
            [
              ":-",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " will use the default if the variable is either unset or empty, whilst "
            ],
            [
              "\\-",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " only uses the default if the variable is unset, but will use the variable if it is set to the empty string:"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "There are two different ways of performing this expansion, which differ in whether the relevant variable is empty or unset. Using "
          },
          {
            "Text": ":-",
            "AttrFlags": 2
          },
          {
            "Text": " will use the default if the variable is either unset or empty, whilst "
          },
          {
            "Text": "\\-",
            "AttrFlags": 2
          },
          {
            "Text": " only uses the default if the variable is unset, but will use the variable if it is set to the empty string:"
          }
        ]
