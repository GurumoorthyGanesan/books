          "title": [
            [
              "#include\u003cstdio.h\u003e\n\nint main()\n{\n    FILE *even, *odds;\n    int n = 10;\n    size_t k = 0;\n\n    even = fopen(\"even.txt\", \"w\");\n    odds = fopen(\"odds.txt\", \"w\");\n\n    for(k = 1; k \u003c n + 1; k++)\n    {\n        k%2==0 ? fprintf(even, \"\\t%5d\\n\", k)\n               : fprintf(odds, \"\\t%5d\\n\", k);\n    }\n    fclose(even);\n    fclose(odds);\n\n    return 0;\n}"
