              "#include \u003cstdlib.h\u003e /* For EXIT_SUCCESS */\n#include \u003cstdio.h\u003e\n#include \u003cstring.h\u003e\n\n\n#define FORTY_STR \"forty\"\n#define TWO_STR \"two\"\n\nint main(void)\n{\n  char str[42] = FORTY_STR TWO_STR;\n  size_t n = sizeof str; /* Take the size not the length. */\n  char * point_to_two = strstr(str, TWO_STR);\n\n  printf(\"'%s'\\n\", str);\n\n  memset(point_to_two, '\\0', n);\n\n  printf(\"'%s'\\n\", str);\n\n  memset(str, '\\0', n);\n\n  printf(\"'%s'\\n\", str);\n\n  return EXIT_SUCCESS;\n}"
