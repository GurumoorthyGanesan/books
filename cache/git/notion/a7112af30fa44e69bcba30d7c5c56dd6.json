              "git_branch() {\n git branch 2\u003e /dev/null | sed -e '/^[^*]/d' -e 's/* \\(.*\\)/ (\\1)/'\n}\nexport PS1=\"\\u@\\h \\[\\033[32m\\]\\w\\[\\033[33m\\]\\$(git_branch)\\[\\033[00m\\] $ \""
            ]
          ]
        },
