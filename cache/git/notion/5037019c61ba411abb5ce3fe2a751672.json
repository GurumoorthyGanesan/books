        "properties": {
          "title": [
            [
              "If you are pulling in fresh commits from the remote repository and you have local changes on the current branch then git will automatically merge the remote version and your version. If you would like to reduce the number of merges on your branch you can tell git to "
            ],
            [
              "rebase",
              [
                [
                  "a",
                  "https://stackoverflow.com/documentation/git/355/rebasing"
                ]
              ]
            ],
            [
              " your commits on the remote version of the branch."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "If you are pulling in fresh commits from the remote repository and you have local changes on the current branch then git will automatically merge the remote version and your version. If you would like to reduce the number of merges on your branch you can tell git to "
          },
          {
            "Text": "rebase",
            "Link": "https://stackoverflow.com/documentation/git/355/rebasing"
          },
          {
            "Text": " your commits on the remote version of the branch."
          }
        ]
