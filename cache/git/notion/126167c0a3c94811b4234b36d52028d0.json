        "properties": {
          "title": [
            [
              "Some text editors (mostly GUI ones) will only run one instance at a time, and generally quit if you already have an instance of them open. If this is the case for your text editor, Git will print the message "
            ],
            [
              "Aborting commit due to empty commit message.",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " without allowing you to edit the commit message first. If this happens to you, consult your text editor’s documentation to see if it has a "
            ],
            [
              "--wait",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " flag (or similar) that will make it pause until the document is closed."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Some text editors (mostly GUI ones) will only run one instance at a time, and generally quit if you already have an instance of them open. If this is the case for your text editor, Git will print the message "
          },
          {
            "Text": "Aborting commit due to empty commit message.",
            "AttrFlags": 2
          },
          {
            "Text": " without allowing you to edit the commit message first. If this happens to you, consult your text editor’s documentation to see if it has a "
          },
          {
            "Text": "--wait",
            "AttrFlags": 2
          },
          {
            "Text": " flag (or similar) that will make it pause until the document is closed."
          }
        ]
      }
