        "code": "export function getAuthToken(storeContainer) {\n    return config =\u003e {\n        let store = storeContainer.getState();\n        config.headers['Authorization'] = store.user.accessToken;\n        return config;\n    };\n}",
        "code_language": "Plain Text"
      },
