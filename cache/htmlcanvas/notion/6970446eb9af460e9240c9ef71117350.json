        "properties": {
          "title": [
            [
              "The answer",
              [
                [
                  "b"
                ]
              ]
            ],
            [
              " is that Canvas actually orders the display to draw a 2 pixel wide line from "
            ],
            [
              "4.0",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " to "
            ],
            [
              "6.0",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ". It also colors the line lighter than the defined "
            ],
            [
              "black",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ". This strange drawing behavior is “anti-aliasing” and it helps Canvas avoid drawing strokes that look jagged."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The answer",
            "AttrFlags": 1
          },
          {
            "Text": " is that Canvas actually orders the display to draw a 2 pixel wide line from "
          },
          {
            "Text": "4.0",
            "AttrFlags": 2
          },
          {
            "Text": " to "
          },
          {
            "Text": "6.0",
            "AttrFlags": 2
          },
          {
            "Text": ". It also colors the line lighter than the defined "
          },
          {
            "Text": "black",
            "AttrFlags": 2
          },
          {
            "Text": ". This strange drawing behavior is “anti-aliasing” and it helps Canvas avoid drawing strokes that look jagged."
          }
        ]
