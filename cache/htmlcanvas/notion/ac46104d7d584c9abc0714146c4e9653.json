        "properties": {
          "title": [
            [
              "Shadows (like gradients) requires extensive computations and therefore you should use shadows sparingly."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Shadows (like gradients) requires extensive computations and therefore you should use shadows sparingly."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
