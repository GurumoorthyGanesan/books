            [
              "The rule for "
            ],
            [
              "COMMENT ON ROLE",
              [
                [
                  "b"
                ],
                [
                  "i"
                ]
              ]
            ],
            [
              " is that you must be superuser to comment on a superuser role, or have the "
            ],
            [
              "CREATEROLE",
              [
                [
                  "b"
                ],
                [
                  "i"
                ]
              ]
            ],
            [
              " privilege to comment on non-superuser roles. Of course, a "
