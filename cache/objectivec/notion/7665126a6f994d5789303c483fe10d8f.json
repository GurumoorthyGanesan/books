              "#import \u003cFoundation/Foundation.h\u003e\n \nint main ()\n{\n   /* local variable definition */\n   int a = 10;\n\n   /* do loop execution */\n   do\n   {\n      if( a == 15)\n      {\n         /* skip the iteration */\n         a = a + 1;\n         continue;\n      }\n      NSLog(@\"value of a: %d\\n\", a);\n      a++;\n     \n   }while( a \u003c 20 );\n \n   return 0;\n}"
