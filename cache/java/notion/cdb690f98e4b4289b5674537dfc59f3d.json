        "properties": {
          "title": [
            [
              "Normally, an object is deleted after it has been finalized. However, this doesn’t happen all of the time. Consider the following example1:"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Normally, an object is deleted after it has been finalized. However, this doesn’t happen all of the time. Consider the following example1:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550461541064,
        "id": "82f1753f-f3fd-4725-a59b-8249fd76892f",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550461541064,
        "parent_id": "cdb690f9-8e4b-4289-b567-4537dfc59f3d",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "public class CaptainJack {\n    public static CaptainJack notDeadYet = null;\n\n    protected void finalize() {\n        // Resurrection!\n        notDeadYet = this;\n    }\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "public class CaptainJack {\n    public static CaptainJack notDeadYet = null;\n\n    protected void finalize() {\n        // Resurrection!\n        notDeadYet = this;\n    }\n}",
        "code_language": "Plain Text"
