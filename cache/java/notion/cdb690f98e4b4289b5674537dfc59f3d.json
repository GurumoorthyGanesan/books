            [
              "finalize",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " method to perform cleanup (or other) actions that must be performed in a timely fashion. Over reliance on finalization can lead to storage leaks, memory leaks and other problems."
            ]
