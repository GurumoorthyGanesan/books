        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Java does not even guarantee that a "
            ],
            [
              "finalize()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " method will be called some time during the running application’s lifetime."
            ]
          ]
        },
        "type": "bulleted_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "Java does not even guarantee that a "
          },
          {
            "Text": "finalize()",
            "AttrFlags": 2
          },
          {
            "Text": " method will be called some time during the running application’s lifetime."
          }
        ]
