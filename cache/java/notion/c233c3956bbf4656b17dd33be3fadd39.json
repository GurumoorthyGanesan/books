        "parent_table": "block",
        "properties": {
          "title": [
            [
              "You cannot put other characters before or after the “*”; e.g. “someFolder/*.jar” is not a wildcard."
            ]
          ]
        },
        "type": "numbered_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "You cannot put other characters before or after the “*”; e.g. “someFolder/*.jar” is not a wildcard."
          }
        ]
