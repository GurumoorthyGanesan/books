        "properties": {
          "title": [
            [
              "Classpath wildcards were first introduced in Java 6. Earlier versions of Java do not treat “*” as a wildcard."
            ]
          ]
        },
        "type": "numbered_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "Classpath wildcards were first introduced in Java 6. Earlier versions of Java do not treat “*” as a wildcard."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
