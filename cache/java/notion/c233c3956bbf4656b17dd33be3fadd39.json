        "properties": {
          "title": [
            [
              "A wildcard matches only files with the suffix “.jar” or “.JAR”. ZIP files are ignored, as are JAR files with a different suffixes."
            ]
          ]
        },
        "type": "numbered_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "A wildcard matches only files with the suffix “.jar” or “.JAR”. ZIP files are ignored, as are JAR files with a different suffixes."
          }
        ]
