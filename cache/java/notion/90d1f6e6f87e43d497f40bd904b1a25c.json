          ],
          "title": [
            [
              "public void writeLineToFile(String str) throws IOException {\n  File file = new File(\"file.txt\");\n  BufferedWriter bw = null;\n  try {\n    bw = new BufferedWriter(new FileWriter(file));\n    bw.write(str);\n  } finally {\n    if (bw != null) {\n      bw.close();\n    }\n  }\n}"
            ]
          ]
        },
