      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550460196274,
        "id": "8e4486fc-8227-4110-b9b2-732ff77d5289",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550460196274,
        "parent_id": "8e6c4c68-b824-4c46-9276-765f586986ff",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "int[] array = new int[] { 1, 2, 3, 4 }; // Original array.\nint[] result = new int[array.length - 1]; // Array which will contain the result.\nint index = 1; // Remove the value \"2\".\n\n// Copy the elements at the left of the index.\nSystem.arraycopy(array, 0, result, 0, index);\n// Copy the elements at the right of the index.\nSystem.arraycopy(array, index + 1, result, index, array.length - index - 1);\n\nSystem.out.println(Arrays.toString(result)); //[1, 3, 4]"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "int[] array = new int[] { 1, 2, 3, 4 }; // Original array.\nint[] result = new int[array.length - 1]; // Array which will contain the result.\nint index = 1; // Remove the value \"2\".\n\n// Copy the elements at the left of the index.\nSystem.arraycopy(array, 0, result, 0, index);\n// Copy the elements at the right of the index.\nSystem.arraycopy(array, index + 1, result, index, array.length - index - 1);\n\nSystem.out.println(Arrays.toString(result)); //[1, 3, 4]",
        "code_language": "Plain Text"
