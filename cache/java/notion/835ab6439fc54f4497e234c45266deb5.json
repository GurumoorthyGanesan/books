        "parent_id": "835ab643-9fc5-4f44-97e2-34c45266deb5",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Upwards compatible changes to other parts of your codebase, to Java standard libraries or to 3rd party libraries can lead to compilation errors."
            ]
          ]
        },
        "type": "bulleted_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "Upwards compatible changes to other parts of your codebase, to Java standard libraries or to 3rd party libraries can lead to compilation errors."
          }
        ]
