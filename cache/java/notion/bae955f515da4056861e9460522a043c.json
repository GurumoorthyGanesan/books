        "parent_table": "block",
        "properties": {
          "title": [
            [
              "This also means that when casting to a generic type (e.g.: "
            ],
            [
              "(List\u003cString\u003e) list",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              "), the cast is an "
            ],
            [
              "unchecked cast",
              [
                [
                  "i"
                ]
              ]
            ],
            [
              ". Because the parameter "
            ],
            [
              "\u003cString\u003e",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " is erased, the JVM cannot check if a cast from a "
            ],
            [
              "List\u003c?\u003e",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " to a "
            ],
            [
              "List\u003cString\u003e",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " is correct; the JVM only sees a cast for "
            ],
            [
              "List",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " to "
            ],
            [
              "List",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " at runtime."
            ]
          ]
        },
        "type": "text",
