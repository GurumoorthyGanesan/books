        "properties": {
          "title": [
            [
              "The Java Language Specification (JLS) classifies the different kinds of Java class as follows:"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The Java Language Specification (JLS) classifies the different kinds of Java class as follows:"
          }
