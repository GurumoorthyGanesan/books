        "parent_table": "block",
        "properties": {
          "title": [
            [
              "An inner class is a nested class that is not explicitly or implicitly declared static."
            ]
          ]
        },
        "type": "quote",
        "version": 1,
        "inline_content": [
          {
            "Text": "An inner class is a nested class that is not explicitly or implicitly declared static."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550463214154,
        "id": "c2765404-ea5d-4868-8e4b-ee17597c0c5e",
