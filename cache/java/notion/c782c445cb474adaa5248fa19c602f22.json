        "parent_id": "c782c445-cb47-4ada-a524-8fa19c602f22",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "A top level class is a class that is not a nested class."
            ]
          ]
        },
        "type": "quote",
        "version": 1,
        "inline_content": [
          {
            "Text": "A top level class is a class that is not a nested class."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550463214154,
        "id": "8fe92fd5-59f1-4ee3-8b8e-d131764eb57d",
