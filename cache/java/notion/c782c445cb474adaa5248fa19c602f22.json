        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550463214154,
        "parent_id": "c782c445-cb47-4ada-a524-8fa19c602f22",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "A nested class is any class whose declaration occurs within the body of another class or interface."
            ]
          ]
        },
        "type": "quote",
        "version": 1,
        "inline_content": [
          {
            "Text": "A nested class is any class whose declaration occurs within the body of another class or interface."
          }
        ]
      },
