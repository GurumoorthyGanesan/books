          "title": [
            [
              "SELECT videogame.id,\n    videogame.title,\n    videogame.reg_date,\n    tags.name,\n    tags_meta.post_id\nFROM tags_meta\nINNER JOIN videogame ON videogame.id = tags_meta.post_id\nINNER JOIN tags ON tags.id = tags_meta.tag_id\nWHERE tags.name = \"elizabeth\"\nORDER BY videogame.reg_date"
            ]
          ]
        },
