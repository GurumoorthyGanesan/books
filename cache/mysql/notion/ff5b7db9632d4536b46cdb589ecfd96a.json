        "parent_id": "ff5b7db9-632d-4536-b46c-db589ecfd96a",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "If two transactions trying to modify the same row and both uses row level locking, one of the transactions waits for the other to complete."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "If two transactions trying to modify the same row and both uses row level locking, one of the transactions waits for the other to complete."
          }
        ]
