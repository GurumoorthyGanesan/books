        "inline_content": [
          {
            "Text": "The capturing of slow queries is either turned on or off. And the file logged to is also specified. The below captures these concepts:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550382616065,
        "id": "10dab691-07f4-4542-a246-327478e77984",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550382616065,
        "parent_id": "c35f7241-4e87-4593-9c3f-2655453448bf",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "SELECT @@slow_query_log; -- Is capture currently active? (1=On, 0=Off)\nSELECT @@slow_query_log_file; -- filename for capture. Resides in datadir\nSELECT @@datadir; -- to see current value of the location for capture file\n\nSET GLOBAL slow_query_log=0; -- Turn Off\n-- make a backup of the Slow Query Log capture file. Then delete it.\nSET GLOBAL slow_query_log=1; -- Turn it back On (new empty file is created)"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "SELECT @@slow_query_log; -- Is capture currently active? (1=On, 0=Off)\nSELECT @@slow_query_log_file; -- filename for capture. Resides in datadir\nSELECT @@datadir; -- to see current value of the location for capture file\n\nSET GLOBAL slow_query_log=0; -- Turn Off\n-- make a backup of the Slow Query Log capture file. Then delete it.\nSET GLOBAL slow_query_log=1; -- Turn it back On (new empty file is created)",
        "code_language": "Plain Text"
      },
