          "title": [
            [
              "/* A C program to implement Bresenham line drawing algorithm for |m|\u003e1 */\n#include\u003cstdio.h\u003e\n#include\u003cconio.h\u003e\n#include\u003cgraphics.h\u003e\n#include\u003cmath.h\u003e\nint main()\n{\nint gdriver=DETECT,gmode;\nint x1,y1,x2,y2,delx,dely,p,i;\ninitgraph(\u0026gdriver,\u0026gmode,\"c:\\\\TC\\\\BGI\");\nprintf(\"Enter the intial points: \");\nscanf(\"%d\",\u0026x1);\nscanf(\"%d\",\u0026y1);\nprintf(\"Enter the end points: \");\nscanf(\"%d\",\u0026x2);\nscanf(\"%d\",\u0026y2);\nputpixel(x1,y1,RED);\ndelx=fabs(x2-x1);\ndely=fabs(y2-y1);\np=(2*delx)-dely;\nfor(i=0;i\u003cdelx;i++){\nif(p\u003c0)\n{\ny1=y1+1;\nputpixel(x1,y1,RED);\np=p+(2*delx);\n}\nelse\n{\nx1=x1+1;\ny1=y1+1;\nputpixel(x1,y1,RED);\np=p+(2*delx)-(2*dely);\n}\n}\ngetch();\nclosegraph();\n return 0;\n}"
            ]
          ]
        },
