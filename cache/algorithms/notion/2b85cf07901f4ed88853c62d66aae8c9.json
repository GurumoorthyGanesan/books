          "title": [
            [
              "/* A C program to implement Bresenham line drawing algorithm for |m|\u003c1 */\n#include\u003cstdio.h\u003e\n#include\u003cconio.h\u003e\n#include\u003cgraphics.h\u003e\n#include\u003cmath.h\u003e\n\nint main()\n{    \n int gdriver=DETECT,gmode;\n int x1,y1,x2,y2,delx,dely,p,i;\n initgraph(\u0026gdriver,\u0026gmode,\"c:\\\\TC\\\\BGI\");\n\nprintf(\"Enter the intial points: \");\nscanf(\"%d\",\u0026x1);\nscanf(\"%d\",\u0026y1);\nprintf(\"Enter the end points: \");\nscanf(\"%d\",\u0026x2);\nscanf(\"%d\",\u0026y2);\n\nputpixel(x1,y1,RED);\n\ndelx=fabs(x2-x1);\ndely=fabs(y2-y1);\np=(2*dely)-delx;\nfor(i=0;i\u003cdelx;i++){\nif(p\u003c0)\n{\n x1=x1+1;\n putpixel(x1,y1,RED);\n p=p+(2*dely);\n}\nelse\n{\n x1=x1+1;\n y1=y1+1;\n putpixel(x1,y1,RED);\n p=p+(2*dely)-(2*delx);\n}\n}\n getch();\n closegraph();\n return 0;\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "/* A C program to implement Bresenham line drawing algorithm for |m|\u003c1 */\n#include\u003cstdio.h\u003e\n#include\u003cconio.h\u003e\n#include\u003cgraphics.h\u003e\n#include\u003cmath.h\u003e\n\nint main()\n{    \n int gdriver=DETECT,gmode;\n int x1,y1,x2,y2,delx,dely,p,i;\n initgraph(\u0026gdriver,\u0026gmode,\"c:\\\\TC\\\\BGI\");\n\nprintf(\"Enter the intial points: \");\nscanf(\"%d\",\u0026x1);\nscanf(\"%d\",\u0026y1);\nprintf(\"Enter the end points: \");\nscanf(\"%d\",\u0026x2);\nscanf(\"%d\",\u0026y2);\n\nputpixel(x1,y1,RED);\n\ndelx=fabs(x2-x1);\ndely=fabs(y2-y1);\np=(2*dely)-delx;\nfor(i=0;i\u003cdelx;i++){\nif(p\u003c0)\n{\n x1=x1+1;\n putpixel(x1,y1,RED);\n p=p+(2*dely);\n}\nelse\n{\n x1=x1+1;\n y1=y1+1;\n putpixel(x1,y1,RED);\n p=p+(2*dely)-(2*delx);\n}\n}\n getch();\n closegraph();\n return 0;\n}",
