            [
              "X[1..m]=abcbdab",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " and "
            ],
            [
              "Y[1..n]=bdcaba",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", the lcs is "
            ],
            [
              "Z[1..r]=bcba",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ". By inserting the non-lcs symbols while preserving the symbol order, we get the scs: "
            ],
            [
              "U[1..t]=abdcabdab",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              "."
            ]
