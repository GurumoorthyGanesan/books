        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "before_filter do\n  user_email = params[:user_email].presence\n  user       = user_email \u0026\u0026 User.find_by_email(user_email)\n\n  if user \u0026\u0026 Devise.secure_compare(user.authentication_token, params[:auth_token])\n    sign_in user, store: false\n  end\nend"
            ]
          ]
        },
