              "// :glot\npackage main\n\nimport \"fmt\"\n\n// :show start\nfunc filterEvenValues(a []int) []int {\n\tvar res []int\n\tfor _, el := range a {\n\t\tif el%2 == 0 {\n\t\t\tcontinue\n\t\t}\n\t\tres = append(res, el)\n\t}\n\treturn res\n}\n\n// :show end\n\nfunc main() {\n\t// :show start\n\ta := []int{1, 2, 3, 4}\n\tres := filterEvenValues(a)\n\tfmt.Printf(\"%#v\\n\", res)\n\t// :show end\n}"
