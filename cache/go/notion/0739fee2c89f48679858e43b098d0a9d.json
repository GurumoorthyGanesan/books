              "// :glot\npackage main\n\nimport (\n\t\"fmt\"\n\t\"time\"\n)\n\n// :show start\n\nfunc main() {\n\tchResult := make(chan int, 1)\n\n\tgo func() {\n\t\ttime.Sleep(1 * time.Second)\n\t\tchResult \u003c- 5\n\t\tfmt.Printf(\"Worker finished\")\n\t}()\n\n\tselect {\n\tcase res := \u003c-chResult:\n\t\tfmt.Printf(\"Got %d from worker\\n\", res)\n\tcase \u003c-time.After(100 * time.Millisecond):\n\t\tfmt.Printf(\"Timed out before worker finished\\n\")\n\t}\n}\n\n// :show end"
