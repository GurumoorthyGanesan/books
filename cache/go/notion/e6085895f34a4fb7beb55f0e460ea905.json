              "// :glot\npackage main\n\nimport \"fmt\"\n\n// :show start\nfunc multipleReturn() (int, int) {\n\treturn 1, 2\n}\n\nfunc multipleReturn2() (a int, b int) {\n\ta = 3\n\tb = 4\n\treturn\n}\n\nfunc main() {\n\tx, y := multipleReturn()  // x = 1, y = 2\n\tw, z := multipleReturn2() // w = 3, z = 4\n\tfmt.Printf(\"x: %d, y: %d\\nw: %d, z: %d\\n\", x, y, w, z)\n\t// :show end\n}"
