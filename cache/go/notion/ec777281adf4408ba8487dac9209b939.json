              "// :glot, no output\npackage main\n\nimport \"fmt\"\n\nfunc main() {\n\t// :show start\n\ts := []int(nil)\n\tif len(s) == 0 {\n\t\tfmt.Printf(\"s  is empty: %#v\\n\", s)\n\t}\n\n\tvar s2 []int\n\tif len(s2) == 0 {\n\t\tfmt.Printf(\"s2 is empty: %#v\\n\", s2)\n\t}\n\n\ts3 := make([]int, 0)\n\tif len(s2) == 0 {\n\t\tfmt.Printf(\"s3 is empty: %#v\\n\", s3)\n\t}\n\n\t// :show end\n}"
            ]
