        "properties": {
          "title": [
            [
              "By default "
            ],
            [
              "go build",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " generates binary that matches the system on which the compiler is running. I.e. if you run it on Linux OS with intel 64-bit CPU, it'll generate an executable for Linux OS and 64-bit Intel CPU."
            ]
          ]
        },
        "type": "text",
        "version": 966,
