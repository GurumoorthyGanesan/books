          "title": [
            [
              "SELECT \n    COUNT(Id) AS ItemsCount,\n    SUM ( CASE \n            WHEN PriceRating = 'Expensive' THEN 1\n            ELSE 0\n          END\n        ) AS ExpensiveItemsCount\nFROM ItemSales"
            ]
          ]
        },
