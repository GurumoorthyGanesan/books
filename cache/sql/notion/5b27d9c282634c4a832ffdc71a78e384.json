            ]
          ],
          "title": [
            [
              "SELECT BusinessEntityID, JobTitle, SickLeaveHours,\n       CUME_DIST() OVER(PARTITION BY JobTitle ORDER BY SickLeaveHours ASC)\n       AS \"Cumulative Distribution\",\n       PERCENTILE_DISC(0.5) WITHIN GROUP(ORDER BY SickLeaveHours)\n          OVER(PARTITION BY JobTitle) AS \"Percentile Discreet\"\nFROM Employee;"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "SELECT BusinessEntityID, JobTitle, SickLeaveHours,\n       CUME_DIST() OVER(PARTITION BY JobTitle ORDER BY SickLeaveHours ASC)\n       AS \"Cumulative Distribution\",\n       PERCENTILE_DISC(0.5) WITHIN GROUP(ORDER BY SickLeaveHours)\n          OVER(PARTITION BY JobTitle) AS \"Percentile Discreet\"\nFROM Employee;",
        "code_language": "Plain Text"
