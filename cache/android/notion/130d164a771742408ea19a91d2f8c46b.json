        "properties": {
          "title": [
            [
              "When binding a viewModel function to a property in xml certain function prefixes like "
            ],
            [
              "get",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " or "
            ],
            [
              "is",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " are dropped. Eg. "
            ],
            [
              "ViewModel::getFormattedText",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " on the ViewModel will become "
            ],
            [
              "@{viewModel.formattedText}",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " when binding it to a property in xml. Similarly with "
            ],
            [
              "ViewModel::isContentVisible",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " -\u003e "
            ],
            [
              "@{viewModel.contentVisible}",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " (Java Bean notation)"
            ]
          ]
        },
        "type": "text",
        "version": 1,
