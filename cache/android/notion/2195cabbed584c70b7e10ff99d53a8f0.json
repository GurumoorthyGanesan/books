        "properties": {
          "title": [
            [
              "To share the file we must provide an identifier for the file. This is done by using a URI (Uniform Resource Identifier)."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "To share the file we must provide an identifier for the file. This is done by using a URI (Uniform Resource Identifier)."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550452554964,
        "id": "ee9c7bca-cdc6-4c94-abd2-cece0068d720",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550452554964,
        "parent_id": "2195cabb-ed58-4c70-b7e1-0ff99d53a8f0",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "// We assume the file we want to load is in the documents/ subdirectory\n// of the internal storage\nFile documentsPath = new File(Context.getFilesDir(), \"documents\");\nFile file = new File(documentsPath, \"sample.pdf\");\n// This can also in one line of course:\n// File file = new File(Context.getFilesDir(), \"documents/sample.pdf\");\n\nUri uri = FileProvider.getUriForFile(getContext(), \"com.mydomain.fileprovider\", file);"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "// We assume the file we want to load is in the documents/ subdirectory\n// of the internal storage\nFile documentsPath = new File(Context.getFilesDir(), \"documents\");\nFile file = new File(documentsPath, \"sample.pdf\");\n// This can also in one line of course:\n// File file = new File(Context.getFilesDir(), \"documents/sample.pdf\");\n\nUri uri = FileProvider.getUriForFile(getContext(), \"com.mydomain.fileprovider\", file);",
        "code_language": "Plain Text"
      },
