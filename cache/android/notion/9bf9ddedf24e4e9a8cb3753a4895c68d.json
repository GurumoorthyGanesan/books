        "properties": {
          "title": [
            [
              "Unlike "
            ],
            [
              "commit()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", which writes its preferences out to persistent storage synchronously, "
            ],
            [
              "apply()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " commits its changes to the in-memory "
            ],
            [
              "SharedPreferences",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " immediately but starts an asynchronous commit to disk and you won’t be notified of any failures. If another editor on this "
            ],
            [
              "SharedPreferences",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " does a regular "
            ],
            [
              "commit()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " while a "
            ],
            [
              "apply()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " is still outstanding, the "
            ],
            [
              "commit()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " will block until all async commits(apply) are completed as well as any other sync commits that may be pending."
            ]
          ]
        },
