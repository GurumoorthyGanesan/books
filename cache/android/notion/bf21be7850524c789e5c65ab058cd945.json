        "parent_table": "block",
        "properties": {
          "title": [
            [
              "The same can be achieved also without a properties file, making the password harder to find:"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The same can be achieved also without a properties file, making the password harder to find:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550443591898,
        "id": "05edda12-8b4e-4ce5-bf57-8884a9cf7b5b",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550443591898,
        "parent_id": "bf21be78-5052-4c78-9e5c-65ab058cd945",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "android {\n\n  signingConfigs {\n    release {\n        storeFile file('/your/keystore/location/key')\n        keyAlias 'your_alias'\n        String ps = System.getenv(\"ps\")\n        if (ps == null) {\n             throw new GradleException('missing ps env variable')\n        }\n        keyPassword ps\n        storePassword ps\n    }\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "android {\n\n  signingConfigs {\n    release {\n        storeFile file('/your/keystore/location/key')\n        keyAlias 'your_alias'\n        String ps = System.getenv(\"ps\")\n        if (ps == null) {\n             throw new GradleException('missing ps env variable')\n        }\n        keyPassword ps\n        storePassword ps\n    }\n}",
        "code_language": "Plain Text"
      },
