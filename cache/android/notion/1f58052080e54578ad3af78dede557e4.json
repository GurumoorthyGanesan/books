        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "You can add the following method to your view and then observe the log output when you touch and move your finger around your view."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550445441136,
        "id": "9a981f0a-1a56-4494-be75-1017b0dfeeb6",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550445441136,
        "parent_id": "1f580520-80e5-4578-ad3a-f78dede557e4",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "@Override\npublic boolean onTouchEvent(MotionEvent event) {\n\n    int x = (int) event.getX();\n    int y = (int) event.getY();\n    int action = event.getAction();\n\n    switch (action) {\n        case MotionEvent.ACTION_DOWN:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_DOWN: x = \" + x + \", y = \" + y);\n            break;\n\n        case MotionEvent.ACTION_MOVE:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_MOVE: x = \" + x + \", y = \" + y);\n            break;\n\n        case MotionEvent.ACTION_UP:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_UP: x = \" + x + \", y = \" + y);\n            break;\n    }\n    return true;\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "@Override\npublic boolean onTouchEvent(MotionEvent event) {\n\n    int x = (int) event.getX();\n    int y = (int) event.getY();\n    int action = event.getAction();\n\n    switch (action) {\n        case MotionEvent.ACTION_DOWN:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_DOWN: x = \" + x + \", y = \" + y);\n            break;\n\n        case MotionEvent.ACTION_MOVE:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_MOVE: x = \" + x + \", y = \" + y);\n            break;\n\n        case MotionEvent.ACTION_UP:\n            Log.i(\"CustomView\", \"onTouchEvent: ACTION_UP: x = \" + x + \", y = \" + y);\n            break;\n    }\n    return true;\n}",
        "code_language": "Plain Text"
