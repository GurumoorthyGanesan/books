        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "#!/bin/sh\nadb shell \"run-as \u003csample.package.id\u003e cat '/data/data/\u003csample.package.id\u003e/$1' \u003e '/sdcard/$1'\"\nadb pull \"/sdcard/$1\"\nadb shell \"rm '/sdcard/$1'\""
            ]
          ]
        },
        "type": "code",
