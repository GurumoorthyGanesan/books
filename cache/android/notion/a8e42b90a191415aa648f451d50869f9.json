          "title": [
            [
              "public static boolean isWithinDay(long timeInMillis, int day) {\n    long diff = System.currentTimeMillis() - timeInMillis;\n\n    float dayCount = (float) (diff / DateUtils.DAY_IN_MILLIS);\n\n    return dayCount \u003c day;\n}"
            ]
          ]
        },
