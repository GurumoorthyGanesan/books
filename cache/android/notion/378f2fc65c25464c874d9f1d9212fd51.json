        "properties": {
          "title": [
            [
              "DEX means Android app’s (APK) executable bytecode files in the form of Dalvik Executable (DEX) files, which contain the compiled code used to run your app."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "DEX means Android app’s (APK) executable bytecode files in the form of Dalvik Executable (DEX) files, which contain the compiled code used to run your app."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550447808901,
