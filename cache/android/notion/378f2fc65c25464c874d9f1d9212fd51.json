        "properties": {
          "title": [
            [
              "What is dex?"
            ]
          ]
        },
        "type": "sub_header",
        "version": 1,
        "inline_content": [
          {
            "Text": "What is dex?"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550447808902,
        "id": "a8d6d921-928e-416e-8c29-6a7dfcbbb094",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550447808902,
        "parent_id": "378f2fc6-5c25-464c-874d-9f1d9212fd51",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Dex is the name of the file format and encoding to which Android Java code is compiled. Early versions of Android would load and execute "
            ],
            [
              "dex",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " binaries directly in a virtual machine named Dalvik. More recent versions of Android use the Android Runtime (ART), which treats "
            ],
            [
              "dex",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " files as an intermediate representation and performs further compilations on it prior to running the application."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Dex is the name of the file format and encoding to which Android Java code is compiled. Early versions of Android would load and execute "
          },
          {
            "Text": "dex",
            "AttrFlags": 2
          },
          {
            "Text": " binaries directly in a virtual machine named Dalvik. More recent versions of Android use the Android Runtime (ART), which treats "
          },
          {
            "Text": "dex",
            "AttrFlags": 2
          },
          {
            "Text": " files as an intermediate representation and performs further compilations on it prior to running the application."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550447808902,
