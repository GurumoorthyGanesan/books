        "parent_id": "378f2fc6-5c25-464c-874d-9f1d9212fd51",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "What to do about it:"
            ]
          ]
        },
        "type": "sub_header",
        "version": 1,
        "inline_content": [
          {
            "Text": "What to do about it:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550447808903,
        "id": "a48b9269-2ce2-4972-80a0-41694db44c96",
        "ignore_block_count": true,
