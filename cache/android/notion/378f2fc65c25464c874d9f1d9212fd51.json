        "parent_table": "block",
        "properties": {
          "title": [
            [
              "To overcome this limit requires configure your app build process to generate more than one DEX file, known as a Multidex."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "To overcome this limit requires configure your app build process to generate more than one DEX file, known as a Multidex."
