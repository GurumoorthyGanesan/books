          "title": [
            [
              "void sendBroadcast (Intent intent, String receiverPermission)\n//for example to send a broadcast to Bcastreceiver receiver\nIntent broadcast = new Intent(this, Bcastreceiver.class);\nsendBroadcast(broadcast, \"org.quadcore.mypermission\");"
            ]
