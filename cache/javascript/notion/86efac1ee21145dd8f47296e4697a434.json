              "function bind (func, obj) { \n    return function () {\n        return func.apply(obj, Array.prototype.slice.call(arguments, 1));\n    }\n}\n\nvar obj = { name: \"Foo\" };\n\nfunction print() {\n    console.log(this.name);\n}\n\nprintObj = bind(print, obj);\n\nprintObj();"
