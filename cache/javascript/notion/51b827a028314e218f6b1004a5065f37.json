          "title": [
            [
              "// replacer as a function\nfunction replacer (key, value) {\n    // Filtering out properties\n    if (typeof value === \"string\") {\n        return\n    }    \n    return value\n}\n\nvar foo = { foundation: \"Mozilla\", model: \"box\", week: 45, transport: \"car\", month: 7 }\nJSON.stringify(foo, replacer)\n// -\u003e '{\"week\": 45, \"month\": 7}'"
            ]
          ]
