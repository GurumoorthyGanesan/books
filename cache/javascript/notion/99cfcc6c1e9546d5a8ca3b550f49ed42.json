        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "function oscillator(time, frequency = 1, amplitude = 1, phase = 0, offset = 0){\n    var t = time * frequency * Math.PI * 2; // get phase at time\n    t += phase * Math.PI * 2; // add the phase offset\n    var v = Math.sin(t); // get the value at the calculated position in the cycle\n    v *= amplitude; // set the amplitude\n    v += offset; // add the offset\n    return v;\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "function oscillator(time, frequency = 1, amplitude = 1, phase = 0, offset = 0){\n    var t = time * frequency * Math.PI * 2; // get phase at time\n    t += phase * Math.PI * 2; // add the phase offset\n    var v = Math.sin(t); // get the value at the calculated position in the cycle\n    v *= amplitude; // set the amplitude\n    v += offset; // add the offset\n    return v;\n}",
        "code_language": "Plain Text"
