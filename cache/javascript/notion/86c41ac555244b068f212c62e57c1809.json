        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "['one', 'two', 'three', 'four'].map(function(value, index, arr) {\n  return value.length;\n});\n// → [3, 3, 5, 4]"
            ]
          ]
        },
        "type": "code",
        "version": 1,
