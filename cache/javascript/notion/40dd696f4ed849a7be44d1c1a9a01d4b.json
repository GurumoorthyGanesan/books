        "parent_table": "block",
        "properties": {
          "title": [
            [
              "When, as the program is parsed from left to right, the end of the input stream of tokens is encountered and the parser is unable to parse the input token stream as a single complete ECMAScript "
            ],
            [
              "Program",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", then a semicolon is automatically inserted at the end of the input stream."
            ]
          ]
        },
        "type": "numbered_list",
        "version": 1,
        "inline_content": [
          {
            "Text": "When, as the program is parsed from left to right, the end of the input stream of tokens is encountered and the parser is unable to parse the input token stream as a single complete ECMAScript "
          },
          {
            "Text": "Program",
            "AttrFlags": 2
          },
          {
            "Text": ", then a semicolon is automatically inserted at the end of the input stream."
          }
