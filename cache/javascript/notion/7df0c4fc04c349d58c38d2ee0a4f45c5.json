          "title": [
            [
              "// Set\nlocalStorage.greet = \"Hi!\"; // Same as: window.localStorage.setItem(\"greet\", \"Hi!\");\n\n// Get\nlocalStorage.greet;         // Same as: window.localStorage.getItem(\"greet\");\n\n// Remove item\ndelete localStorage.greet;  // Same as: window.localStorage.removeItem(\"greet\");\n\n// Clear storage\nlocalStorage.clear();"
            ]
          ]
        },
        "type": "code",
