              "var getErrorCode = function(err) {\n  switch (err.code) {\n    case err.PERMISSION_DENIED:\n      return \"PERMISSION_DENIED\";\n    case err.POSITION_UNAVAILABLE:\n      return \"POSITION_UNAVAILABLE\";\n    case err.TIMEOUT:\n      return \"TIMEOUT\";\n    default:\n      return \"UNKNOWN_ERROR\";\n  }\n};"
