              "request.onload = function() {\n  if (this.status \u003e= 200 \u0026\u0026 this.status \u003c 300) {\n    if(request.response) {\n      // Assuming a successful call returns JSON\n      resolve(JSON.parse(request.response));\n    } else {\n      resolve();\n  } else {\n    reject({\n      'status': this.status,\n      'message': request.statusText\n    });\n  }\n};\n\nrequest.onerror = function() {\n  reject({\n    'status': this.status,\n    'message': request.statusText\n  });\n};"
            ]
          ]
