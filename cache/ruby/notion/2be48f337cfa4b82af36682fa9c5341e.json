          "title": [
            [
              "class Dinosaur\n    @base_sound = \"rawrr\"\n\n    def initialize(sound = nil)\n        @sound = sound || self.class.base_sound\n    end\n\n    def speak\n        @sound\n    end\n\n    def try_to_speak\n        @base_sound\n    end\n\n    def count_and_store_sound_length\n        @sound.chars.each_with_index do |char, i|\n            @sound_length = i + 1\n            p \"#{char}: #{sound_length}\"\n        end\n    end\n    \n    def sound_length\n        @sound_length\n    end\n\n    def self.base_sound\n        @base_sound\n    end\nend\n\ndino_1 = Dinosaur.new\ndino_2 = Dinosaur.new \"grrr\"\n\nDinosaur.base_sound\n# =\u003e \"rawrr\"\ndino_2.speak\n# =\u003e \"grrr\""
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "class Dinosaur\n    @base_sound = \"rawrr\"\n\n    def initialize(sound = nil)\n        @sound = sound || self.class.base_sound\n    end\n\n    def speak\n        @sound\n    end\n\n    def try_to_speak\n        @base_sound\n    end\n\n    def count_and_store_sound_length\n        @sound.chars.each_with_index do |char, i|\n            @sound_length = i + 1\n            p \"#{char}: #{sound_length}\"\n        end\n    end\n    \n    def sound_length\n        @sound_length\n    end\n\n    def self.base_sound\n        @base_sound\n    end\nend\n\ndino_1 = Dinosaur.new\ndino_2 = Dinosaur.new \"grrr\"\n\nDinosaur.base_sound\n# =\u003e \"rawrr\"\ndino_2.speak\n# =\u003e \"grrr\"",
