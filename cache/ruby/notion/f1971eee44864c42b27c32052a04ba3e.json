          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "def join_as_string(arg)\n  if arg.instance_of?(Array)\n    arg.join(',')\n  elsif arg.instance_of?(Range)\n    arg.to_a.join(',')\n  else\n    arg.to_s\n  end\nend\n\njoin_as_string('something') #=\u003e \"something\"\njoin_as_string([2, 1, 5])   #=\u003e \"2,1,5\"\njoin_as_string(1)           #=\u003e \"1\"\njoin_as_string(2..4)        #=\u003e \"2,3,4\"\njoin_as_string([])          #=\u003e \"\"\njoin_as_string(nil)         #=\u003e \"\""
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "def join_as_string(arg)\n  if arg.instance_of?(Array)\n    arg.join(',')\n  elsif arg.instance_of?(Range)\n    arg.to_a.join(',')\n  else\n    arg.to_s\n  end\nend\n\njoin_as_string('something') #=\u003e \"something\"\njoin_as_string([2, 1, 5])   #=\u003e \"2,1,5\"\njoin_as_string(1)           #=\u003e \"1\"\njoin_as_string(2..4)        #=\u003e \"2,3,4\"\njoin_as_string([])          #=\u003e \"\"\njoin_as_string(nil)         #=\u003e \"\"",
