        "properties": {
          "title": [
            [
              "The choice of namespace for your extension method class is a trade-off between visibility and discoverability."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The choice of namespace for your extension method class is a trade-off between visibility and discoverability."
          }
