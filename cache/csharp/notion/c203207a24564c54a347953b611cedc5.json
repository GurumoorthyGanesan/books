              "using System;\n                \npublic class Program\n{\n    public static void Main()\n    {\n        var theString = \"hello\";\n        var method = theString\n                     .GetType()\n                     .GetMethod(\"Substring\",\n                                new[] {typeof(int), typeof(int)}); //The types of the method arguments\n         var result = method.Invoke(theString, new object[] {0, 4});\n         Console.WriteLine(result);\n    }\n}"
            ]
          ]
        },
