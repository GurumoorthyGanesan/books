        "properties": {
          "title": [
            [
              "The "
            ],
            [
              "Instance",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " property is assigned by a static constructor that the C# compiler generates. The .NET runtime guarantees that the static constructor is run at most once and is run before "
            ],
            [
              "Instance",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " is first read. Therefore, all synchronization and initialization concerns are carried out by the runtime."
            ]
          ]
