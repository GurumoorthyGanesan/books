        "properties": {
          "title": [
            [
              "A common pattern that can be used when accessing your data through an ADO.NET connection might look as follows :"
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "A common pattern that can be used when accessing your data through an ADO.NET connection might look as follows :"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1549529669400,
        "id": "7688d0d5-c843-49d7-a90a-a2a8e6cc1e11",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550221020000,
        "parent_id": "0cb9c785-f0fe-40e3-870f-789490af3891",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "C#"
            ]
          ],
          "title": [
            [
              "// This scopes the connection (your specific class may vary)\nusing(var connection = new SqlConnection(\"{your-connection-string}\")\n{\n    // Build your query\n    var query = \"SELECT * FROM YourTable WHERE Property = @property\");\n    // Scope your command to execute\n    using(var command = new SqlCommand(query, connection))\n    {\n         // Open your connection\n         connection.Open();\n\n         // Add your parameters here if necessary\n\n         // Execute your query as a reader (again scoped with a using statement)\n         using(var reader = command.ExecuteReader())\n         {\n               // Iterate through your results here\n         }\n    }\n}"
            ]
          ]
        },
        "type": "code",
        "version": 7,
        "code": "// This scopes the connection (your specific class may vary)\nusing(var connection = new SqlConnection(\"{your-connection-string}\")\n{\n    // Build your query\n    var query = \"SELECT * FROM YourTable WHERE Property = @property\");\n    // Scope your command to execute\n    using(var command = new SqlCommand(query, connection))\n    {\n         // Open your connection\n         connection.Open();\n\n         // Add your parameters here if necessary\n\n         // Execute your query as a reader (again scoped with a using statement)\n         using(var reader = command.ExecuteReader())\n         {\n               // Iterate through your results here\n         }\n    }\n}",
        "code_language": "C#"
      },
