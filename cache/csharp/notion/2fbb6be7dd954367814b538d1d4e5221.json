        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "T Read\u003cT\u003e(byte[] buffer)\n    {\n        T result = default(T);\n        \n        var gch = GCHandle.Alloc(buffer, GCHandleType.Pinned);\n    \n        try\n        {\n            result = Marshal.PtrToStructure\u003cT\u003e(gch.AddrOfPinnedObject());\n        }\n        finally\n        {\n            gch.Free();\n        }\n        \n        return result;\n    }"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "T Read\u003cT\u003e(byte[] buffer)\n    {\n        T result = default(T);\n        \n        var gch = GCHandle.Alloc(buffer, GCHandleType.Pinned);\n    \n        try\n        {\n            result = Marshal.PtrToStructure\u003cT\u003e(gch.AddrOfPinnedObject());\n        }\n        finally\n        {\n            gch.Free();\n        }\n        \n        return result;\n    }",
        "code_language": "Plain Text"
