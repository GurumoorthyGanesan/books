        "properties": {
          "language": [
            [
              "C#"
            ]
          ],
          "title": [
            [
              "Observable\n     .FromEventPattern(textBoxInput, \"TextChanged\")\n     .Select(s =\u003e ((TextBox) s.Sender).Text)\n     .Throttle(TimeSpan.FromSeconds(0.5))\n     .DistinctUntilChanged()\n     .Subscribe(text =\u003e Console.WriteLine(text));"
            ]
          ]
        },
