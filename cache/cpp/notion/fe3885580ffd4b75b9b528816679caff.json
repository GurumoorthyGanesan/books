        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "A class or a structure may declare any function it’s friend. If a function is a friend of a class, it may access all it’s protected and private members:"
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550523388472,
        "id": "ca7c4a69-3e2a-414e-991e-29167280858f",
