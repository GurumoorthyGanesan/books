        "last_edited_time": 1550801640000,
        "parent_id": "e3a18fb3-6346-4a5d-8c2d-612c8cce98e2",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "C++"
            ]
          ],
          "title": [
            [
              "class base1\n{\n  public:\n     void funtion( )\n     { //code for base1 function }  \n};\nclass base2\n{\n    void function( )\n     { // code for base2 function } \n};\n\nclass derived : public base1, public base2\n{\n    \n};\n\nint main()\n{\n    derived obj;\n    \n  // Error because compiler can't figure out which function to call \n  //either function( ) of base1 or base2 .   \n    obj.function( )  \n}"
            ]
          ]
        },
        "type": "code",
        "version": 3,
        "code": "class base1\n{\n  public:\n     void funtion( )\n     { //code for base1 function }  \n};\nclass base2\n{\n    void function( )\n     { // code for base2 function } \n};\n\nclass derived : public base1, public base2\n{\n    \n};\n\nint main()\n{\n    derived obj;\n    \n  // Error because compiler can't figure out which function to call \n  //either function( ) of base1 or base2 .   \n    obj.function( )  \n}",
        "code_language": "C++"
      },
