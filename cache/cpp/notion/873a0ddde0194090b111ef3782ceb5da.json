        "properties": {
          "title": [
            [
              "Singletons aren’t concurrency friendly. When a class has a global point of access, every thread has the ability to access it which can lead to deadlocks and race conditions."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Singletons aren’t concurrency friendly. When a class has a global point of access, every thread has the ability to access it which can lead to deadlocks and race conditions."
          }
        ]
      },
