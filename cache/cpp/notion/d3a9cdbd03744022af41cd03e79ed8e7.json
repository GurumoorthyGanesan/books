        "properties": {
          "language": [
            [
              "Makefile"
            ]
          ],
          "title": [
            [
              "CXX=g++\nCXXFLAGS=-Wall -Wextra -pedantic\nEXE=app\n\nSRCS_GLOB=src/*.cpp\nSRCS=$(wildcard $(SRCS_GLOB))\nOBJS=$(SRCS:.cpp=.o)\n\nall: $(EXE)\n\n$(EXE): $(OBJS)\n    @$(CXX) -o $@ $(OBJS)\n\ndepend: .depend\n\n.depend: $(SRCS)\n    @-rm -f ./.depend\n    @$(CXX) $(CXXFLAGS) -MM $^\u003e\u003e./.depend\n\nclean:\n    -rm -f $(EXE)\n    -rm $(OBJS)\n    -rm *~\n    -rm .depend\n\ninclude .depend"
            ]
          ]
        },
        "type": "code",
        "version": 3,
        "code": "CXX=g++\nCXXFLAGS=-Wall -Wextra -pedantic\nEXE=app\n\nSRCS_GLOB=src/*.cpp\nSRCS=$(wildcard $(SRCS_GLOB))\nOBJS=$(SRCS:.cpp=.o)\n\nall: $(EXE)\n\n$(EXE): $(OBJS)\n    @$(CXX) -o $@ $(OBJS)\n\ndepend: .depend\n\n.depend: $(SRCS)\n    @-rm -f ./.depend\n    @$(CXX) $(CXXFLAGS) -MM $^\u003e\u003e./.depend\n\nclean:\n    -rm -f $(EXE)\n    -rm $(OBJS)\n    -rm *~\n    -rm .depend\n\ninclude .depend",
        "code_language": "Makefile"
