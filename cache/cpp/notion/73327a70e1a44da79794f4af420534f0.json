        "parent_table": "block",
        "properties": {
          "title": [
            [
              "A leading "
            ],
            [
              "::",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " forces the new or delete operator to be looked up in global scope, overriding any overloaded class-specific new or delete operators."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "A leading "
          },
          {
            "Text": "::",
            "AttrFlags": 2
          },
          {
            "Text": " forces the new or delete operator to be looked up in global scope, overriding any overloaded class-specific new or delete operators."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550523049312,
        "id": "7cbff0bf-c5ee-4479-ae27-01200663994a",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550523049312,
        "parent_id": "73327a70-e1a4-4da7-9794-f4af420534f0",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "The optional arguments following the "
            ],
            [
              "new",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " keyword are usually used to call "
            ],
            [
              "placement new",
              [
                [
                  "a",
                  "http://stackoverflow.com/documentation/c%2B%2B/2873/memory-management/9740/placement-new"
                ]
              ]
            ],
            [
              ", but can also be used to pass additional information to the allocator, such as a tag requesting that memory be allocated from a chosen pool."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The optional arguments following the "
          },
          {
            "Text": "new",
            "AttrFlags": 2
          },
          {
            "Text": " keyword are usually used to call "
          },
          {
            "Text": "placement new",
            "Link": "http://stackoverflow.com/documentation/c%2B%2B/2873/memory-management/9740/placement-new"
          },
          {
            "Text": ", but can also be used to pass additional information to the allocator, such as a tag requesting that memory be allocated from a chosen pool."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1550523049313,
        "id": "7a58f555-693e-4bda-a15b-2597343fc138",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1550523049313,
        "parent_id": "73327a70-e1a4-4da7-9794-f4af420534f0",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "The type allocated is usually explicitly specified, "
            ],
            [
              "e.g.,",
              [
                [
                  "i"
                ]
              ]
            ],
            [
              " "
            ],
            [
              "new Foo",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", but can also be written as "
            ],
            [
              "auto",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " (since C++11) or "
            ],
            [
              "decltype(auto)",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " (since C++14) to deduce it from the initializer."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The type allocated is usually explicitly specified, "
          },
          {
            "Text": "e.g.,",
            "AttrFlags": 4
          },
          {
            "Text": " "
          },
          {
            "Text": "new Foo",
            "AttrFlags": 2
          },
          {
            "Text": ", but can also be written as "
          },
