        "parent_id": "42e24f22-45a0-4e56-9e04-b81c8c6993b1",
        "parent_table": "block",
        "properties": {
          "title": [
            [
              "File.Move(@\"C:\\TemporaryFile.txt\", @\"C:\\TemporaryFiles\\TemporaryFile.txt\");",
              [
                [
                  "c"
                ]
              ]
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "File.Move(@\"C:\\TemporaryFile.txt\", @\"C:\\TemporaryFiles\\TemporaryFile.txt\");",
            "AttrFlags": 2
          }
        ]
      },
