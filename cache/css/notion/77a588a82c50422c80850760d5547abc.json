        "parent_table": "block",
        "properties": {
          "title": [
            [
              "When an element has been set to be "
            ],
            [
              "display:none;",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " the browser ignores every other layout property for that specific element (both "
            ],
            [
              "position",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " and "
            ],
            [
              "float",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              "). No box will be rendered for that element and its existence in html does not affect the position of following elements."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "When an element has been set to be "
          },
          {
            "Text": "display:none;",
            "AttrFlags": 2
          },
          {
            "Text": " the browser ignores every other layout property for that specific element (both "
          },
          {
            "Text": "position",
            "AttrFlags": 2
          },
          {
            "Text": " and "
          },
          {
            "Text": "float",
            "AttrFlags": 2
          },
          {
            "Text": "). No box will be rendered for that element and its existence in html does not affect the position of following elements."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
