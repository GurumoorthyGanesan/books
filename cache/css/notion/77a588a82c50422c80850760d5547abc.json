        "parent_table": "block",
        "properties": {
          "title": [
            [
              "Note that this is different from setting the "
            ],
            [
              "visibility",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " property to "
            ],
            [
              "hidden",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ". Setting "
            ],
            [
              "visibility: hidden;",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " for an element would not display the element on the page but the element would still take up the space in the rendering process as if it would be visible. This will therefore affect how following elements are displayed on the page."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Note that this is different from setting the "
          },
          {
            "Text": "visibility",
            "AttrFlags": 2
          },
          {
            "Text": " property to "
          },
          {
            "Text": "hidden",
            "AttrFlags": 2
          },
          {
            "Text": ". Setting "
          },
          {
            "Text": "visibility: hidden;",
            "AttrFlags": 2
          },
          {
            "Text": " for an element would not display the element on the page but the element would still take up the space in the rendering process as if it would be visible. This will therefore affect how following elements are displayed on the page."
          }
        ]
