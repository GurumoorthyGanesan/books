              "class ColorPicker {\n    private $colors = [\"#FF0064\", \"#0064FF\", \"#64FF00\", \"#FF6400\", \"#00FF64\", \"#6400FF\"];\n    public function nextColor() : string {\n        $result = next($colors);\n        // if end of array reached\n        if (key($colors) === null) {\n            reset($colors);\n        }\n        return $result;\n    }\n}"
