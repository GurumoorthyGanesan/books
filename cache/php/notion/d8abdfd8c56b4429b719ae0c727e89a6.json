              "\u003c?php\n\nfunction randomNumbers(int $length)\n{\n    for ($i = 0; $i \u003c $length; $i++) {\n        // yield tells the PHP interpreter that this value\n        // should be the one used in the current iteration.\n        yield mt_rand(1, 10);\n    }\n}\n\nforeach (randomNumbers(10) as $number) {\n    echo \"$number\\n\";\n}"
