          ],
          "title": [
            [
              "$arr = ['a' =\u003e \"AA\", 'b' =\u003e \"BB\", 'c' =\u003e \"CC\"];\n\necho implode(\" \", array_map(function($key, $val) { \n    return \"$key:$val\"; // function that glues key to the value\n}, array_keys($arr), $arr)); \n\n// Output: a:AA b:BB c:CC"
            ]
          ]
