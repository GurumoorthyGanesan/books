          "title": [
            [
              "for ($i = 0; $i \u003c= 9; $i++) {\n    echo $i, ',';\n}\n\n# Example 2\nfor ($i = 0; ; $i++) {\n  if ($i \u003e 9) {\n      break;\n  }\n  echo $i, ',';\n}\n\n# Example 3\n$i = 0;\nfor (; ; ) {\n    if ($i \u003e 9) {\n        break;\n    }\n    echo $i, ',';\n    $i++;\n}\n\n# Example 4\nfor ($i = 0, $j = 0; $i \u003c= 9; $j += $i, print $i. ',', $i++);"
            ]
          ]
        },
