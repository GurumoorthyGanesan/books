              "const SOURCE_FOLDER = __DIR__ . \"/src\";\nspl_autoload_register(function (string $className) {\n    $file = getClassPath(SOURCE_FOLDER, $className);\n    if (is_readable($file)) require_once $file;\n});"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "const SOURCE_FOLDER = __DIR__ . \"/src\";\nspl_autoload_register(function (string $className) {\n    $file = getClassPath(SOURCE_FOLDER, $className);\n    if (is_readable($file)) require_once $file;\n});",
        "code_language": "Plain Text"
