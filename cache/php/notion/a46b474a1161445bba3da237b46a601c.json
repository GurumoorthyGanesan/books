        "code": "function getClassPath(string $sourceFolder, string $className, string $extension = \".php\") {\n    return $sourceFolder . \"/\" . str_replace(\"\\\\\", \"/\", $className) . $extension; // note that \"/\" works as a directory separator even on Windows\n}",
