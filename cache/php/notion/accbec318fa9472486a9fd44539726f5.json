        "parent_id": "accbec31-8fa9-4724-86a9-fd44539726f5",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$my_array = [1,2,3,4,5,6,7,8];\n\n$even_numbers = array_filter($my_array, function($number) {\n    return $number % 2 === 0;   \n});"
