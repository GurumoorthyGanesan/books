            ],
            [
              "/home/user/file.txt",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", while on Windows, absolute paths start with the drive, e.g. "
            ],
            [
              "C:/Users/user/file.txt",
              [
                [
                  "c"
                ]
              ]
            ]
          ]
        },
