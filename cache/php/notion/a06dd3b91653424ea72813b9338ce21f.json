        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "$now = new DateTime();// empty argument returns the current date\n$interval = new DateInterval('P7D');//this objet represents a 7 days interval\n$lastDay = $now-\u003eadd($interval); //this will return a DateTime object\n$formatedLastDay = $lastDay-\u003eformat('Y-m-d');//this method format the DateTime object and returns a String\necho \"Samara says: Seven Days. You'll be happy on $formatedLastDay.\";"
            ]
          ]
        },
        "type": "code",
