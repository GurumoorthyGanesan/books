          {
            "Text": "imap_list",
            "Link": "https://secure.php.net/manual/en/function.imap-list.php"
          },
          {
            "Text": ". The first parameter is the resource you acquired from "
          },
          {
            "Text": "imap_open",
            "AttrFlags": 2
          },
          {
            "Text": ", the second is your mailbox string and the third is a fuzzy search string ("
          },
          {
            "Text": "\\*",
            "AttrFlags": 2
          },
          {
            "Text": " is used to match any pattern)."
          }
        ]
