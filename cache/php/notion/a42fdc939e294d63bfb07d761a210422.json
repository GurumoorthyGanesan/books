          "title": [
            [
              "The following example scans files in a directory, deletes member files/directories recursively, and returns the number of files (not directories) deleted."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "The following example scans files in a directory, deletes member files/directories recursively, and returns the number of files (not directories) deleted."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "created_time": 1552022661092,
        "id": "6ccdca75-39a1-43ec-9400-8bc14fdeb5bd",
        "ignore_block_count": true,
        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1552022661092,
        "parent_id": "a42fdc93-9e29-4d63-bfb0-7d761a210422",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "function recurse_delete_dir(string $dir) : int {\n    $count = 0;\n\n    // ensure that $dir ends with a slash so that we can concatenate it with the filenames directly\n    $dir = rtrim($dir, \"/\\\\\") . \"/\";\n\n    // use dir() to list files\n    $list = dir($dir);\n\n    // store the next file name to $file. if $file is false, that's all -- end the loop.\n    while(($file = $list-\u003eread()) !== false) {\n        if($file === \".\" || $file === \"..\") continue;\n        if(is_file($dir . $file)) {\n            unlink($dir . $file);\n            $count++;\n        } elseif(is_dir($dir . $file)) {\n            $count += recurse_delete_dir($dir . $file);\n        }\n    }\n\n    // finally, safe to delete directory!\n    rmdir($dir);\n\n    return $count;\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "function recurse_delete_dir(string $dir) : int {\n    $count = 0;\n\n    // ensure that $dir ends with a slash so that we can concatenate it with the filenames directly\n    $dir = rtrim($dir, \"/\\\\\") . \"/\";\n\n    // use dir() to list files\n    $list = dir($dir);\n\n    // store the next file name to $file. if $file is false, that's all -- end the loop.\n    while(($file = $list-\u003eread()) !== false) {\n        if($file === \".\" || $file === \"..\") continue;\n        if(is_file($dir . $file)) {\n            unlink($dir . $file);\n            $count++;\n        } elseif(is_dir($dir . $file)) {\n            $count += recurse_delete_dir($dir . $file);\n        }\n    }\n\n    // finally, safe to delete directory!\n    rmdir($dir);\n\n    return $count;\n}",
        "code_language": "Plain Text"
      }
