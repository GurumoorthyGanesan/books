        "last_edited_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
        "last_edited_time": 1552022703179,
        "parent_id": "4b9510e9-75ff-47e5-9b7a-c76a953411e1",
        "parent_table": "block",
        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "function gen_one_to_three() {\n    $keys = [\"first\", \"second\", \"third\"];\n\n    for ($i = 1; $i \u003c= 3; $i++) {\n        // Note that $i is preserved between yields.\n        yield $keys[$i - 1] =\u003e $i;\n    }\n}\n\nforeach (gen_one_to_three() as $key =\u003e $value) {\n    echo \"$key: $value\\n\";\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "function gen_one_to_three() {\n    $keys = [\"first\", \"second\", \"third\"];\n\n    for ($i = 1; $i \u003c= 3; $i++) {\n        // Note that $i is preserved between yields.\n        yield $keys[$i - 1] =\u003e $i;\n    }\n}\n\nforeach (gen_one_to_three() as $key =\u003e $value) {\n    echo \"$key: $value\\n\";\n}",
        "code_language": "Plain Text"
