          "title": [
            [
              "class CloneableUser\n{\n    public $name;\n    public $lastName;\n\n    /**\n     * This method will be invoked by a clone operator and will prepend \"Copy \" to the\n     * name and lastName properties.\n     */\n    public function __clone()\n    {\n        $this-\u003ename = \"Copy \" . $this-\u003ename;\n        $this-\u003elastName = \"Copy \" . $this-\u003elastName;\n    }\n}"
            ]
          ]
        },
        "type": "code",
