        "parent_table": "block",
        "properties": {
          "title": [
            [
              "All PCRE strings in PHP must be enclosed with delimiters. A delimiter can be any non-alphanumeric, non-backslash, non-whitespace character. Popular delimiters are "
            ],
            [
              "~",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", "
            ],
            [
              "/",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              ", "
            ],
            [
              "%",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " for instance."
            ]
          ]
        },
        "type": "text",
