        "properties": {
          "title": [
            [
              "PHP regular expressions follow PCRE pattern standards, which are derived from Perl regular expressions."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "PHP regular expressions follow PCRE pattern standards, which are derived from Perl regular expressions."
          }
        ]
      },
