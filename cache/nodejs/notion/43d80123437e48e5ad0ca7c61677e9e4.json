              "Plain Text"
            ]
          ],
          "title": [
            [
              "http.createServer(function (request, response) {\n    \n    if (request.url in routes) {\n        return routes[request.url](request, response);\n    }\n\n    response.writeHead(404);\n    response.end(http.STATUS_CODES[404]);\n\n}).listen(1337);"
            ]
          ]
        },
