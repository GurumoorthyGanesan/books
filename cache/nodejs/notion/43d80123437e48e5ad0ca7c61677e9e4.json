        "properties": {
          "language": [
            [
              "Plain Text"
            ]
          ],
          "title": [
            [
              "var routes = {\n    '/': function index (request, response) {\n        response.writeHead(200);\n        response.end('Hello, World!');\n    },\n    '/foo': function foo (request, response) {\n        response.writeHead(200);\n        response.end('You are now viewing \"foo\"');\n    }\n}"
            ]
          ]
        },
        "type": "code",
        "version": 1,
        "code": "var routes = {\n    '/': function index (request, response) {\n        response.writeHead(200);\n        response.end('Hello, World!');\n    },\n    '/foo': function foo (request, response) {\n        response.writeHead(200);\n        response.end('You are now viewing \"foo\"');\n    }\n}",
        "code_language": "Plain Text"
      },
