              "// greet.js\nconst express = require('express');\n\nmodule.exports = function(options = {}) { // Router factory\n    const router = express.Router();\n    // Get controller\n    const {service} = options;\n\n    router.get('/greet', (req, res, next) =\u003e {\n        res.end(\n            service.createGreeting(req.query.name || 'Stranger')\n        );\n    });\n\n    return router;\n};"
            ]
          ]
        },
