          ],
          "title": [
            [
              "client.hmset('frameworks', 'javascript', 'AngularJS', 'css', 'Bootstrap', 'node', 'Express');\n\nclient.hgetall('frameworks', function(err, object) {\n    console.log(object);\n});"
