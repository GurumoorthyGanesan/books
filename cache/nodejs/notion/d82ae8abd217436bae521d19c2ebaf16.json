              "$(document).ready(function(){\n    $(\"form#changeQuote\").on('submit', function(e){\n        e.preventDefault();\n        var data = $('input[name=quote]').val();\n        $.ajax({\n            type: 'post',\n            url: '/ajax',\n            data: data,\n            dataType: 'text'\n        })\n        .done(function(data){\n            $('h1').html(data.quote);\n        });\n    });\n});"
            ]
          ]
        },
