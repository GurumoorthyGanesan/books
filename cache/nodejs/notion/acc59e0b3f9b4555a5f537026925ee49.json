        "properties": {
          "title": [
            [
              "Note that "
            ],
            [
              "cluster.fork()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " spawns a child process that begins executing the current script from the beginning, in contrast to the "
            ],
            [
              "fork()",
              [
                [
                  "c"
                ]
              ]
            ],
            [
              " system call in "
            ],
            [
              "C",
              [
                [
                  "i"
                ]
              ]
            ],
            [
              " which clones the current process and continues from the instruction after the system call in both parent and child process."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Note that "
          },
          {
            "Text": "cluster.fork()",
            "AttrFlags": 2
          },
          {
            "Text": " spawns a child process that begins executing the current script from the beginning, in contrast to the "
          },
          {
            "Text": "fork()",
            "AttrFlags": 2
          },
          {
            "Text": " system call in "
          },
          {
            "Text": "C",
            "AttrFlags": 4
          },
          {
            "Text": " which clones the current process and continues from the instruction after the system call in both parent and child process."
          }
        ]
