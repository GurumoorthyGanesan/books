        "properties": {
          "language": [
            [
              "Shell"
            ]
          ],
          "title": [
            [
              "@echo off\nsetlocal EnableDelayedExpansion\nfor /l %%i in (1, 1, 10) do (\n  set array_%%i=!random!\n)\n\nfor /l %%i in (1, 1, 10) do (\n  echo !array_%%i!\n)\n\n:: For each item in the array, not knowing the length\nset i=1\n:startloop\nif not defined array_%i% goto endloop\nset array_%i%=!array_%i%!_dummy_suffix\necho A%i%: !array_%i%!\nset /a i+=1\ngoto startloop\n:endloop"
            ]
          ]
        },
